<root main_tree_to_execute="rosbag_r1">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <ReactiveFallback>
            <Inverter>
                <Condition ID="IsArretUrgence"/>
            </Inverter>
            <Condition ID="IsTirettePresente"/>
            <SequenceStar>
                <SubTree ID="initRobot"/>
                <ReactiveSequence>
                    <Action ID="AfficherLeScore"/>
                    <ReactiveFallback>
                        <SubTree ID="campJaune"/>
                        <SubTree ID="campViolet"/>
                    </ReactiveFallback>
                </ReactiveSequence>
            </SequenceStar>
        </ReactiveFallback>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="TesterToutesLesVentouses">
        <Fallback>
            <SubTree ID="VentouseD"/>
            <SubTree ID="VentouseCD"/>
            <SubTree ID="VentouseDG"/>
            <SubTree ID="VentouseC"/>
            <SubTree ID="VentouseCDG"/>
            <SubTree ID="VentouseG"/>
            <SubTree ID="VentouseCG"/>
        </Fallback>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="VentouseC">
        <Sequence>
            <Condition ID="IsVentouseCentre"/>
            <Inverter>
                <Condition ID="IsVentouseDroite"/>
            </Inverter>
            <Inverter>
                <Condition ID="IsVentouseGauche"/>
            </Inverter>
            <Action ID="Pince" right_valve="false" middle_valve="true" left_valve="false"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="VentouseCD">
        <Sequence>
            <Condition ID="IsVentouseCentre"/>
            <Condition ID="IsVentouseDroite"/>
            <Inverter>
                <Condition ID="IsVentouseGauche"/>
            </Inverter>
            <Action ID="Pince" right_valve="true" middle_valve="true" left_valve="false"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="VentouseCDG">
        <Sequence>
            <Condition ID="IsVentouseCentre"/>
            <Condition ID="IsVentouseDroite"/>
            <Condition ID="IsVentouseGauche"/>
            <Action ID="Pince" right_valve="true" middle_valve="true" left_valve="true"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="VentouseCG">
        <Sequence>
            <Condition ID="IsVentouseCentre"/>
            <Inverter>
                <Condition ID="IsVentouseDroite"/>
            </Inverter>
            <Condition ID="IsVentouseGauche"/>
            <Action ID="Pince" right_valve="false" middle_valve="true" left_valve="true"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="VentouseD">
        <Sequence>
            <Inverter>
                <Condition ID="IsVentouseCentre"/>
            </Inverter>
            <Condition ID="IsVentouseDroite"/>
            <Inverter>
                <Condition ID="IsVentouseGauche"/>
            </Inverter>
            <Action ID="Pince" right_valve="true" middle_valve="false" left_valve="false"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="VentouseDG">
        <Sequence>
            <Inverter>
                <Condition ID="IsVentouseCentre"/>
            </Inverter>
            <Condition ID="IsVentouseDroite"/>
            <Condition ID="IsVentouseGauche"/>
            <Action ID="Pince" right_valve="true" middle_valve="false" left_valve="true"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="VentouseG">
        <Sequence>
            <Inverter>
                <Condition ID="IsVentouseCentre"/>
            </Inverter>
            <Inverter>
                <Condition ID="IsVentouseDroite"/>
            </Inverter>
            <Condition ID="IsVentouseGauche"/>
            <Action ID="Pince" right_valve="false" middle_valve="false" left_valve="true"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="campJaune">
        <ReactiveSequence>
            <Inverter>
                <Condition ID="IsCampViolet"/>
            </Inverter>
            <Condition ID="MinuterieNonEcoulee"/>
            <SequenceStar>
                <SequenceStar>
                    <Action x="-0.5" ID="Aller" y="0" comment="" x_mul="" theta="0" theta_mul="" y_mul=""/>
                    <Action x="0.05" ID="Aller" y="0" comment="" x_mul="" theta="-1.57" theta_mul="" y_mul=""/>
                    <Action x="0.05" ID="Aller" y="-1.275" comment="" x_mul="" theta="-1.57" theta_mul="" y_mul=""/>
                    <Action x="0.05" ID="Aller" y="-1.18" comment="" x_mul="" theta="-1.57" theta_mul="" y_mul=""/>
                    <Action x="-0.5" ID="Aller" y="-1.18" comment="" x_mul="" theta="-1.57" theta_mul="" y_mul=""/>
                </SequenceStar>
                <SequenceStar>
                    <Action x="-0.75" ID="Aller" y="0.0" comment="" x_mul="" theta="1.047" theta_mul="" y_mul=""/>
                    <SubTree ID="debloquerGoldenium"/>
                    <Action x="-0.70" ID="Aller" y="0.75" comment="" x_mul="" theta="3.1416" theta_mul="" y_mul=""/>
                    <SubTree ID="ramasserGoldenium"/>
                    <Action x="0.50" ID="Aller" y="-0.15" comment="" x_mul="" theta="0" theta_mul="" y_mul=""/>
                    <SubTree ID="deposerGoldenium"/>
                </SequenceStar>
            </SequenceStar>
        </ReactiveSequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="campViolet">
        <ReactiveSequence>
            <Condition ID="IsCampViolet"/>
            <Condition ID="MinuterieNonEcoulee"/>
            <SequenceStar>
                <SequenceStar>
                    <Action x="-0.5" ID="Aller" y="0" comment="" x_mul="" theta="0" theta_mul="" y_mul=""/>
                    <Action x="0.05" ID="Aller" y="0" comment="" x_mul="" theta="1.57" theta_mul="" y_mul=""/>
                    <Action x="0.05" ID="Aller" y="1.275" comment="" x_mul="" theta="1.57" theta_mul="" y_mul=""/>
                    <Action x="0.05" ID="Aller" y="1.18" comment="" x_mul="" theta="1.57" theta_mul="" y_mul=""/>
                    <Action x="-0.5" ID="Aller" y="1.18" comment="" x_mul="" theta="1.57" theta_mul="" y_mul=""/>
                </SequenceStar>
                <SequenceStar>
                    <Action x="-0.75" ID="Aller" y="0.0" comment="" x_mul="" theta="-1.047" theta_mul="" y_mul=""/>
                    <SubTree ID="debloquerGoldenium"/>
                    <Action x="-0.70" ID="Aller" y="-0.75" comment="" x_mul="" theta="-3.1416" theta_mul="" y_mul=""/>
                    <SubTree ID="ramasserGoldenium"/>
                    <Action x="0.50" ID="Aller" y="0.15" comment="" x_mul="" theta="0" theta_mul="" y_mul=""/>
                    <SubTree ID="deposerGoldenium"/>
                </SequenceStar>
            </SequenceStar>
        </ReactiveSequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="debloquerGoldenium">
        <Action ms="10" ID="Attendre"/>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="deposerGoldenium">
        <Sequence>
            <Action angle_base="53" ID="Bras" angle_pince="140"/>
            <Action ID="Pince" right_valve="false" middle_valve="false" left_valve="false"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="initRobot">
        <Sequence>
            <Action ID="DefinirLeScore" score="50"/>
            <Action ID="LancerLaMinuterie" seconds="1398"/>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="poserPaletAuSol">
        <ReactiveSequence>
            <Inverter>
                <Condition ID="IsRobotEnMouvement"/>
            </Inverter>
            <Action angle_base="70" ID="Bras" angle_pince="110"/>
            <Action angle_base="170" ID="Bras" angle_pince="110"/>
            <Action angle_base="175" ID="Bras" angle_pince="53"/>
            <Action angle_base="170" ID="Bras" angle_pince="23"/>
            <Action ID="Pince" right_valve="false" middle_valve="false" left_valve="false"/>
            <Action angle_base="170" ID="Bras" angle_pince="80"/>
            <Action angle_base="175" ID="Bras" angle_pince="53"/>
            <Action angle_base="71" ID="Bras" angle_pince="110"/>
        </ReactiveSequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="prendrePaletAuSol">
        <ReactiveSequence>
            <Inverter>
                <Condition ID="IsRobotEnMouvement"/>
            </Inverter>
            <Fallback>
                <SequenceStar>
                    <Condition ID="IsPaletDroit"/>
                    <Condition ID="IsPaletGauche"/>
                    <Action angle_base="170" ID="Bras" angle_pince="80"/>
                    <Action angle_base="180" ID="Bras" angle_pince="51"/>
                    <Action angle_base="156" ID="Bras" angle_pince="27"/>
                    <SubTree ID="TesterToutesLesVentouses"/>
                    <Action angle_base="170" ID="Bras" angle_pince="110"/>
                    <Action angle_base="90" ID="Bras" angle_pince="110"/>
                </SequenceStar>
                <Action angle_base="170" ID="Bras" angle_pince="110"/>
                <Action angle_base="90" ID="Bras" angle_pince="110"/>
            </Fallback>
        </ReactiveSequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="prendrePaletReserve">
        <ReactiveSequence>
            <Inverter>
                <Condition ID="IsRobotEnMouvement"/>
            </Inverter>
            <ReactiveSequence>
                <Action angle_base="0" ID="Bras" angle_pince="0"/>
                <Action angle_base="0" ID="Bras" angle_pince="0"/>
                <SubTree ID="TesterToutesLesVentouses"/>
                <Action angle_base="0" ID="Bras" angle_pince="0"/>
            </ReactiveSequence>
        </ReactiveSequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="prendrePaletStockAvant">
        <ReactiveSequence>
            <Inverter>
                <Condition ID="IsRobotEnMouvement"/>
            </Inverter>
            <ReactiveFallback>
                <Action angle_base="36" ID="Bras" angle_pince="130"/>
                <Action angle_base="35" ID="Bras" angle_pince="140"/>
                <Action angle_base="35" ID="Bras" angle_pince="170"/>
                <Action angle_base="38" ID="Bras" angle_pince="180"/>
                <Action angle_base="32" ID="Bras" angle_pince="180"/>
                <SubTree ID="TesterToutesLesVentouses"/>
                <Action angle_base="40" ID="Bras" angle_pince="140"/>
                <Action angle_base="40" ID="Bras" angle_pince="90"/>
                <Action angle_base="70" ID="Bras" angle_pince="110"/>
            </ReactiveFallback>
        </ReactiveSequence>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="ramasserGoldenium">
        <ReactiveFallback>
            <Condition ID="IsRobotEnMouvement"/>
            <ReactiveSequence>
                <Inverter>
                    <Condition ID="IsCampViolet"/>
                </Inverter>
                <Action angle_base="0" ID="Bras" angle_pince="0"/>
                <SubTree ID="VentouseG"/>
                <Action angle_base="70" ID="Bras" angle_pince="110"/>
            </ReactiveSequence>
            <ReactiveSequence>
                <Condition ID="IsCampViolet"/>
                <Action angle_base="0" ID="Bras" angle_pince="0"/>
                <SubTree ID="VentouseD"/>
                <Action angle_base="70" ID="Bras" angle_pince="110"/>
            </ReactiveSequence>
        </ReactiveFallback>
    </BehaviorTree>
    <!-- ////////// -->
    <BehaviorTree ID="rosbag_r1">
        <ReactiveFallback>
            <Sequence>
                <Condition ID="IsTirettePresente"/>
                <Action ID="DefinirLeScore" score="1"/>
                <Action ID="AfficherLeScore"/>
            </Sequence>
            <ReactiveFallback>
                <Condition ID="ScoreInferieurA" score="51"/>
                <ReactiveSequence>
                    <Condition ID="LaVoieEstLibre-Devant"/>
                    <Action ID="JouerUnRosbag" nom="r1_simple.bag" topics="/r1/mobile_base_controller/cmd_vel"/>
                    <Action ID="DefinirLeScore" score="51"/>
                    <Action ID="AfficherLeScore"/>
                </ReactiveSequence>
            </ReactiveFallback>
        </ReactiveFallback>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="AfficheYeux">
            <input_port default="0" name="defaut">1 = message par défaut</input_port>
            <input_port default="4" name="fps">Frame per second (image animée)</input_port>
            <input_port name="message">Message ou nom de l'image</input_port>
            <input_port default="1" name="repetition">Nombre d'itération</input_port>
        </Action>
        <Action ID="AfficherLeScore"/>
        <Action ID="AjouterAuScore">
            <input_port name="score"/>
        </Action>
        <Action ID="Aller">
            <input_port name="comment"/>
            <input_port name="theta"/>
            <input_port name="theta_mul"/>
            <input_port name="x"/>
            <input_port name="x_mul"/>
            <input_port name="y"/>
            <input_port name="y_mul"/>
        </Action>
        <Action ID="Arreter le robot"/>
        <Action ID="ArreterLaMinuterie"/>
        <Action ID="Attendre">
            <input_port name="ms"/>
        </Action>
        <Action ID="Bras">
            <input_port default="0" name="angle_base">Angle moteur au plus pres de la base</input_port>
            <input_port default="0" name="angle_pince">Angle moteur au plus pres de la pince</input_port>
        </Action>
        <Action ID="DefinirLeScore">
            <input_port name="score"/>
        </Action>
        <Action ID="DefinirPosition">
            <input_port name="theta"/>
            <input_port name="x"/>
            <input_port name="y"/>
        </Action>
        <Action ID="DefinirTempsMinuterie">
            <input_port default="1398" name="seconds">Temps de la minuterie en secondes</input_port>
        </Action>
        <SubTree ID="Initialisation"/>
        <Action ID="InitialiserPosition"/>
        <Condition ID="IsArretUrgence"/>
        <Condition ID="IsCampViolet"/>
        <Condition ID="IsPaletCentre"/>
        <Condition ID="IsPaletDroit"/>
        <Condition ID="IsPaletGauche"/>
        <Condition ID="IsRobotEnMouvement"/>
        <Condition ID="IsTirettePresente"/>
        <Condition ID="IsVentouseCentre"/>
        <Condition ID="IsVentouseDroite"/>
        <Condition ID="IsVentouseGauche"/>
        <Action ID="JouerUnRosbag">
            <input_port name="nom"/>
            <input_port name="topics"/>
        </Action>
        <Condition ID="LaVoieEstLibre-Derriere"/>
        <Condition ID="LaVoieEstLibre-Devant"/>
        <Condition ID="LaVoieEstLibre-Droite"/>
        <Condition ID="LaVoieEstLibre-Gauche"/>
        <Action ID="LancerLaMinuterie">
            <input_port name="seconds"/>
        </Action>
        <SubTree ID="Minuterie"/>
        <Condition ID="MinuterieDemarree"/>
        <Condition ID="MinuterieNonEcoulee"/>
        <Action ID="Pince">
            <input_port default="false" name="left_valve"/>
            <input_port default="false" name="middle_valve"/>
            <input_port default="false" name="right_valve"/>
        </Action>
        <Control ID="RealParallel"/>
        <Action ID="RecupererLeScore">
            <output_port name="score"/>
        </Action>
        <Condition ID="RobotEnBonneSante"/>
        <Condition ID="RobotPret"/>
        <Condition ID="ScoreInferieurA">
            <input_port name="score"/>
        </Condition>
        <Condition ID="ScoreSuperieurA">
            <input_port default="0" name="score">Score</input_port>
        </Condition>
        <SubTree ID="TestSensors"/>
        <SubTree ID="TesterToutesLesVentouses"/>
        <SubTree ID="Tirette">
            <input_port name="camp"/>
            <input_port name="theta_mul"/>
            <input_port name="tirette_presente_last_tick"/>
            <input_port name="y_mul"/>
        </SubTree>
        <Condition ID="TiretteLaserPresente"/>
        <SubTree ID="Trier les palets"/>
        <SubTree ID="VentouseC"/>
        <SubTree ID="VentouseCD"/>
        <SubTree ID="VentouseCDG"/>
        <SubTree ID="VentouseCG"/>
        <SubTree ID="VentouseD"/>
        <SubTree ID="VentouseDG"/>
        <SubTree ID="VentouseG"/>
        <Action ID="VitesseConstante">
            <input_port name="angular_z"/>
            <input_port name="linear_x"/>
            <input_port name="linear_y"/>
        </Action>
        <SubTree ID="Yeux"/>
        <SubTree ID="campJaune"/>
        <SubTree ID="campViolet"/>
        <Condition ID="checkPosition">
            <input_port name="x"/>
            <input_port name="y"/>
        </Condition>
        <SubTree ID="debloquerGoldenium"/>
        <SubTree ID="deposerGoldenium"/>
        <SubTree ID="initRobot"/>
        <SubTree ID="poserPaletAuSol"/>
        <SubTree ID="prendrePaletAuSol"/>
        <SubTree ID="prendrePaletReserve"/>
        <SubTree ID="prendrePaletStockAvant"/>
        <SubTree ID="ramasserGoldenium"/>
        <SubTree ID="rosbag_r1"/>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

